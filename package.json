{
  "name": "usdc-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "start": "nuxt start",
    "lint": "vue-cli-service lint",
    "test": "jest"
  },
  "dependencies": {
    "@testing-library/vue": "^5.0.4",
    "@vue/cli-plugin-unit-jest": "^4.5.6",
    "@vue/test-utils": "^1.1.0",
    "axios": "^0.20.0",
    "core-js": "^3.6.5",
    "moment": "^2.29.0",
    "nuxt": "^2.14.6",
    "sass": "^1.26.11",
    "vue": "^2.6.11",
    "vue-material": "^1.0.0-beta-15",
    "web3": "^1.3.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.0",
    "@vue/cli-plugin-eslint": "^4.5.0",
    "@vue/cli-service": "^4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-vue": "^6.2.2",
    "eslint-plugin-vue-scoped-css": "^1.0.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^10.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended",
      "plugin:jest/recommended",
      "plugin:vue-scoped-css/recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "indent": [
        "error",
        2
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "eol-last": "error",
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "vue-scoped-css/no-unused-selector": [
        "off"
      ],
      "vue/this-in-template": [
        "off"
      ],
      "vue/require-default-prop": [
        "off"
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
